{
  "name": "urbandictionarybot",
  "author": "vontus",
  "license": "MIT",
  "version": "0.1.0",
  "description": "An Urban Dictionary bot for Telegram",
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "pretty": "prettier --write .",
    "start": "node -r ts-node/register src/index.ts",
    "start:inspect": "node --inspect=0.0.0.0:6068 -r dotenv/config -r ts-node/register src/index.ts",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.11",
    "@types/jsonfile": "^6.1.4",
    "@types/lodash": "^4.14.202",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^20.10.4",
    "@types/node-schedule": "^2.1.5",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/string-template": "^1.0.6",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "1.0.0-rc.12",
    "debug": "^4.3.4",
    "fs": "0.0.1-security",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "moment": "^2.29.4",
    "node-schedule": "^2.1.1",
    "node-telegram-bot-api": "^0.64.0",
    "path": "^0.12.7",
    "string-template": "^1.0.0",
    "ts-node": "^10.9.2",
    "yamljs": "^0.3.0"
  },
  "jest-junit": {
    "outputDirectory": "coverage"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix --max-warnings=0",
    "*.{css,md,json}": "prettier --write --ignore-unknown"
  }
